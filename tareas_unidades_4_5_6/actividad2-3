diary on
t = (0:.1:2)';
y =  [5.8955 3.5639 2.5173 1.9790 1.8990 1.3938 1.1359 ...
1.0096 1.0343 0.8435 0.6856 0.6100 0.5392 0.3946 ...
0.3903 0.5474 0.3459 0.1370 0.2211 0.1704 0.2636]';
x = (0:0.01:2)';
X=[t.^0 t.^1 t.^2];
b=X\y;
yq=(b'*[x.^0 x.^1 x.^2]')'; 
plot(t,y,'*',x,yq);
plot(t,y,'*',x,yq);
X=[t.^0 t.^1];
b=X\log(y);
yq=exp(b'*[x.^0 x.^1]')'; 
figure;
plot(t,y,'o',x,yq);
diary off
